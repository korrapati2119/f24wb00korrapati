block content
  h1= korrapati
  p Welcome to korrapati
  button(type="button", class="btn btn-secondary", onclick="boost()") press me
  div#boost add in extra

  script.
    let value = 0;
    let amt = 3;

    function boost(){
      amt += 1;
      value += amt;
      
      document.getElementById("boost").innerText = `Value boosted by ${value} by ${amt}`;
    }
    a(href='/vehicles') View Vehicles